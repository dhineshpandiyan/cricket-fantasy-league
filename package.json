{
  "name": "cricket-fantasy-league",
  "version": "1.0.0",
  "description": "Simple app for cricket fantasy league",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "generate-rank": "node src/server/rank-generator.js",
    "unit-test": "node ./node_modules/.bin/mocha --require spec/init --require mocha-junit --opts ./mocha-unit.opts"
  },
  "author": "Dhinesh Pandiyan Muniyasamy",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.0.1",
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "baobab": "^2.5.2",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "express": "^4.16.3",
    "moment": "^2.24.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-facebook-login": "^4.1.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.0.0",
    "chai": "4.1.x",
    "chai-as-promised": "7.1.x",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "3.x",
    "mocha-junit": "^0.4.0",
    "mocha-junit-reporter": "1.13.x",
    "mocha-multi": "0.11.x",
    "nodemon": "^1.17.3",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.2.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
